{
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2016"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@global/*": ["global/*"],
      "@hooks/*": ["hooks/*"],
      "@mocks/*": ["mocks/*"],
      "@navigations/*": ["navigations/*"],
      "@pages/*": ["pages/*"],
      "@screens/*": ["screens/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"]
    },
    "types": ["styled-components-react-native"]
  },
  "exclude": [
    "expo/tsconfig.base",
    "node_modules",
    "bable.config.js",
    "metro.config.js",
    "jest.config,js"
  ],
  "extends": "expo/tsconfig.base"
}
