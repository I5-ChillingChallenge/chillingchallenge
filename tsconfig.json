{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2016"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./src",

    "types": ["styled-components-react-native"]
  },
  "exclude": [
    "expo/tsconfig.base",
    "node_modules",
    "bable.config.js",
    "metro.config.js",
    "jest.config,js"
  ],
  "extends": "expo/tsconfig.base"
}
